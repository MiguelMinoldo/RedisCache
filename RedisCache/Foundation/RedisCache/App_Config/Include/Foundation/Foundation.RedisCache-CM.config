<!--For more information on using transformations see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <sitecore>
    <events xdt:Transform="Insert">
      <event name="item:saved">
        <handler type="Foundation.RedisCache.Events.ItemSavedRedirectHandler, Foundation.UrlRewriting" method="OnItemSavedRedirect" />
      </event>
      <event name="customCache:rebuild">
        <handler type="Foundation.RedisCache.Events.EventHandlers.CacheRebuildEventHandler, Foundation.UrlRewriting" method="OnCustomCacheRebuild" />
      </event>
      <event name="item:saving">
        <handler type="Foundation.RedisCache.Events.TrimRewriteUrlRelatedTitlesHandler, Foundation.UrlRewriting" method="OnItemSaving" />
      </event>
    </events>
  </sitecore>
</configuration>
